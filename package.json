{
  "name": "tmpl-vue-exp-gcp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode production --skip-plugins pwa --watch",
    "build-pwa": "vue-cli-service build --mode pwaProduction --watch",
    "start": "node index.js",
    "mount": "nodemon --ignore src/ --ignore public/ --ignore dist/ index.js",
    "dev": "concurrently \"npm run mount\" \"npm run serve\" \"npm run build\"",
    "deploy": "gcloud app deploy"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@google-cloud/logging": "^7.3.0",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "cors": "^2.8.4",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "querystring": "~0.2.0",
    "register-service-worker": "^1.7.1",
    "request": "~2.83.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}